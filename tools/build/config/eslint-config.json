{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "window": true,
    "React": true,
    "ReactDOM": true,
    "document": true,
    "alert": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "arrow-parens": 0,
    "no-global-assign": 0,
    "no-unsafe-negation": 0,
    "no-console": 0,
    "spaced-comment": 0,
    "react/react-in-jsx-scope": 0,
    "react/no-deprecated": 0,
    "no-alert": 0
  }
}